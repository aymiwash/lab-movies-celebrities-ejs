<h2>Create a new movie</h2>

<form action="/movies/create" method="POST">
  <label>Title: <input type="text" name="title" /></label>
  <label>Genre: <input type="text" name="genre" /></label>
  <label>Plot: <input type="text" name="plot" /></label>
  <label
    >Cast:
    <select name="cast" id="cast" multiple>
      <% allCelebrities.forEach(celebrity => { %>
       <option value="<%- celebrity._id %>">I<%- celebrity.name %></option>
      <% }) %>
      
    </select>
  </label>

  <button type="submit">Create new movie</button>
</form>
